{
  "name": "markdown",
  "version": "1.0.0",
  "description": "Online markdown editor",
  "main": "index.html",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --optimize-minimize",
    "build:dev": "NODE_ENV=development webpack -d",
    "dev": "webpack-dev-server --mode=development",
    "format": "prettier --write --single-quote --print-width=120 --parser=flow --tab-width=2 \"js/**/*.{js,jsx}\"",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "start": "NODE_ENV=server node server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al1s/Markdown.git"
  },
  "keywords": [
    "FreeCodeCamp"
  ],
  "author": "alstof",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/al1s/Markdown/issues"
  },
  "homepage": "https://github.com/al1s/Markdown#readme",
  "jest": {
    "snapshotSerializers": [
      "jest-serializer-enzyme"
    ]
  },
